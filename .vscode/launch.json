{
    "version": "0.2.0",
    "configurations": [



    
        {
            "name": "PyTest - test_hebbian",
            "type": "debugpy",
            "request": "launch",
            "module": "pytest",
            "args": [
                "${workspaceFolder}\\test\\unit\\core\\optimizers\\test_hebbian.py"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "PyTest - test_exact_match_accuracy",
            "type": "debugpy",
            "request": "launch",
            "module": "pytest",
            "args": [
                "${workspaceFolder}\\test\\unit\\core\\metrics\\test_exact_match_accuracy.py"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "PyTest - test_match_accuracy",
            "type": "debugpy",
            "request": "launch",
            "module": "pytest",
            "args": [
                "${workspaceFolder}\\test\\unit\\core\\metrics\\test_match_accuracy.py"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Run Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": true
        },
        {
            "name": "Run HPO",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}\\scripts\\run_hpo.py",
            "args": [
                "--dataset_config", ".\\demo\\dataset.yaml",
                "--preprocessing_config", ".\\demo\\preprocessing.yaml",
                "--hpo_config", ".\\demo\\hpo.yaml",
                "--training_recipe_config", ".\\demo\\trainer_hpo.yaml",
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": true
        },
        {
            "name": "Run HPO - custom configs",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}\\scripts\\run_hpo.py",
            "args": [
                "--dataset_config", "${input:getSparseyConfigDirectoryString}\\dataset.yaml",
                "--preprocessing_config", "${input:getSparseyConfigDirectoryString}\\preprocessing.yaml",
                "--hpo_config", "${input:getSparseyConfigDirectoryString}\\hpo.yaml",
                "--training_recipe_config", "${input:getSparseyConfigDirectoryString}\\trainer_hpo.yaml",
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": true
        },
        {
            "name": "Train Model",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}\\scripts\\train_model.py",
            "args": [
                "--dataset_config", ".\\demo\\dataset.yaml",
                "--preprocessing_config", ".\\demo\\preprocessing.yaml",
                "--model_config", ".\\demo\\network.yaml",
                "--training_recipe_config", ".\\demo\\trainer.yaml",
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": true
        },
        {
            "name": "PyTest - test_weight_freezing",
            "type": "debugpy",
            "request": "launch",
            "module": "pytest",
            "args": [
                "${workspaceFolder}\\test\\system\\weight_freezing\\test_weight_freezing.py"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "PyTest - test sparsey dataset configs",
            "type": "debugpy",
            "request": "launch",
            "module": "pytest",
            "args": [
                "${workspaceFolder}\\test\\unit\\cli\\config_validation\\dataset\\test_sparsey_dataset_configs.py"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "PyTest - choose file",
            "type": "debugpy",
            "request": "launch",
            "module": "pytest",
            "args": [
                "${workspaceFolder}\\test\\${input:testCaseStringPicker}"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ],
    "inputs": [
        {
            "id": "getSparseyConfigDirectoryString",
            "type": "promptString",
            "description": "enter the path to the directory containing your HPO config files",
            "default": ".\\demo"
        },
        {
            "id": "testCaseStringPicker",
            "type": "pickString",
            "description": "select a test file",
            "options": [
                "integration\\test_hpo_step.py",
                "system\\test_hpo_run.py",
                "system\\weight_freezing\\test_weight_freezing.py",
                "unit\\access_objects\\datasets\\test_sparsey_dataset.py",
                "unit\\access_objects\\models\\test_model_builder.py",
                "unit\\access_objects\\models\\test_model.py",
                "unit\\cli\\config_validation\\dataset\\test_sparsey_dataset_configs.py",
                "unit\\cli\\config_validation\\model\\test_sparsey_model_configs.py",
                "unit\\cli\\config_validation\\trainer\\test_sparsey_trainer_configs.py",
                "unit\\core\\hpo_objectives\\test_hpo_objective.py",
                "unit\\core\\metrics\\test_exact_match_accuracy.py",
                "unit\\core\\metrics\\test_match_accuracy.py",
                "unit\\core\\metrics\\test_metric_factory.py",
                "unit\\core\\model_layers\\test_layer_factory.py",
                "unit\\core\\model_layers\\test_sparsey_layer.py",
                "unit\\core\\model_layers\\test_weight_freeze.py",
                "unit\\core\\optimizers\\test_hebbian.py"
            ]
        }
    ]
}